{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "indent": [true, "spaces", 2],
    "interface-name": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "trailing-comma": [false, "always"]
  }
}
